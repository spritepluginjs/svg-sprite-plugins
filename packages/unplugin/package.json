{
  "name": "@spritepluginjs/svg-unplugin",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.0",
  "description": "Unified plugin system for svg sprite",
  "license": "MIT",
  "homepage": "https://github.com/spritepluginjs/svg-sprite-plugins/packages/unplugin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spritepluginjs/svg-sprite-plugins.git"
  },
  "bugs": {
    "url": "https://github.com/spritepluginjs/svg-sprite-plugins/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "svg-sprite",
    "sprite",
    "svgo",
    "svg"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.cjs",
      "import": "./dist/types.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "start": "esno src/index.ts",
    "test": "vitest",
    "lint-staged": "lint-staged",
    "lint-staged:lint": "eslint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue .",
    "prettier": "prettier --check --write --no-plugin-search \"**/*.(js|jsx|tsx|ts|css|less|scss|sass|md|yaml|vue)\""
  },
  "dependencies": {
    "@spritepluginjs/svg-core": "workspace:^",
    "chokidar": "^3.5.3",
    "consola": "^3.2.3",
    "dedent": "^1.2.0",
    "fs-extra": "^11.1.1",
    "globby": "^13.2.2",
    "lodash": "^4.17.21",
    "minimatch": "^9.0.3",
    "pathe": "^1.1.1",
    "svg-sprite": "^2.0.2",
    "type-fest": "^4.0.0",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.16",
    "@types/svg-sprite": "^0.0.34",
    "@vitejs/plugin-react": "^4.0.3",
    "chalk": "^5.2.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vinyl": "^3.0.0",
    "vite": "^4.3.9",
    "vitest": "^0.31.4",
    "webpack": "^5.85.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": "npm run lint-staged:lint",
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,yaml}": [
      "prettier --write --no-plugin-search"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
